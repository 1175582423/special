<div class="pageContent">
	<form method="post" action="/admin/articleType_save"
		class="pageForm required-validate"
		onsubmit="return validateCallback(this,dialogAjaxDone)">
		<div class="pageFormContent" layoutH="56">
				<p>
					<label>类型名称:</label>
					<input name="dec" class="required" type="text" size="30" />
					<input name="navTabId" type="hidden" value="${(navTabId)!}"/>
				</p>
				<p>
					<label>类型简称:</label>
					<input name="name" class="required error" minlength="4" type="text" size="30" />
					<span class="error" for="name">长度不能大于 四个汉字</span>
				</p>
		</div>
			
		<div class="formBar">
			<ul>
				<li><div class="buttonActive">
						<div class="buttonContent">
							<button type="submit">保存</button>
						</div>
					</div></li>
				<li><div class="button">
						<div class="buttonContent">
							<button type="button" class="close">取消</button>
						</div>
					</div></li>
			</ul>
		</div>
	</form>
</div>