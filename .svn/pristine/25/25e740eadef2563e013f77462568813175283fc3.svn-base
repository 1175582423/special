<div class="pageContent">
	<form method="post" action="/admin/masterplatCssUpdate"
		class="pageForm required-validate"
		onsubmit="return validateCallback(this,dialogAjaxDone)">
		<div class="pageFormContent" layoutH="56">
				<p>
				<label>id:</label>
				<input name="id" readonly="true" class="textInput readonly" type="text" size="30" value="${(model.id)}"/>
				</p>
				<p>
					<label>名称:</label>
					<input name="dec" class="required" type="text" size="30" value="${(model.dec)}"/>
				</p>
				<p>
					<label>代码:</label>
					<input name="name" class="required" type="text" size="30" value="${(model.name)}"/>
				</p>
		</div>
			
		<div class="formBar">
			<ul>
				<li><div class="buttonActive">
						<div class="buttonContent">
							<button type="submit">保存更新</button>
						</div>
					</div></li>
				<li><div class="button">
						<div class="buttonContent">
							<button type="button" class="close">取消</button>
						</div>
					</div></li>
			</ul>
		</div>
	</form>
</div>